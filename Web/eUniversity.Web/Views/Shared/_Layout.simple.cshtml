<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>eUniverity</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <meta name="description" content="Live preview eUniversity">

    @Styles.Render("~/bootstrap/css", "~/ace/css", "~/ace/font")
</head>
<body>
    <header>
        @Html.Partial("_Header")
    </header>
    <div class="main-container" id="main-container">
        <div class="main-container-inner">
            @Html.Partial("_Sidebar")

            <div class="main-content">
                @Html.Partial("_Breadcrumbs")
                <div class="page-content">
                    @Html.Partial("_PageHeader")
                    <div class="row">
                        <div class="col-xs-12">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery", "~/bundles/bootstrap", "~/bundles/ace")
    @RenderSection("scripts", required: false)
</body>
</html>
