@using eUniversity.Web.Infrastructure.Constants
<script type="text/javascript">
    (function () {
        var strings = {
            common: {
                Draft: '@eUniversity.Common.Strings.Resources.Common_Draft',
                Approved: '@eUniversity.Common.Strings.Resources.Common_Approved'
            }
        };
        window.strings = strings;

        var actions = {
            specialization: {
                EditSpecializationUrl: '@Url.Action(ActionNames.EditAction, ControllerNames.SpecializationController, new {Id = "id"})',
                SaveSpecializationUrl: '@Url.Action(ActionNames.EditAction, ControllerNames.SpecializationController)',
            },
            group: {
                EditGroupUrl: '@Url.Action(ActionNames.EditAction, ControllerNames.GroupController, new {Id = "id"})',
                SaveGroupUrl: '@Url.Action(ActionNames.EditAction, ControllerNames.GroupController)'
            }
        };
        window.actions = actions;
    })();
</script>